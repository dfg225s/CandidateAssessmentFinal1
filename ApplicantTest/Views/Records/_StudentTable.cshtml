@model IEnumerable<Student>

<table class="table table-hover table-striped table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.LastName, @student.FirstName</td>
                <td>@student.Email</td>
                <td>@student.Age</td>
            </tr>
        }
    </tbody>
</table>